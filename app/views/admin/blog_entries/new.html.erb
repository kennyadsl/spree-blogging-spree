<h1>New Blog Entry</h1>

<% form_for([:admin, @blog_entry], :html => { :multipart => true }) do |f| %>
  <%= render :partial => f %>
  <p class="form-buttons">
    <%= button t("actions.create"), nil, 'submit', {'class' => 'wymupdate'} %>
    <%= t("or") %> <%= link_to t("actions.cancel"), collection_url %>
  </p>
<% end %>
